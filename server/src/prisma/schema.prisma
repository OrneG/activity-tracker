generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Activity {
  id     Int    @id @default(autoincrement())
  title  String
  type   String
  userId Int
}

model Record {
  id           Int      @id @default(autoincrement())
  calification Int
  date         DateTime
  activityId   Int

  @@unique([date, activityId])
}